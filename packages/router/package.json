{
  "name": "@your-riot/router",
  "version": "0.0.0",
  "description": "Riot router",
  "scripts": {
    "build": "tsc && rolldown -c rolldown.config.ts",
    "cov": "c8 report --reporter=lcov",
    "cov-html": "c8 report --reporter=html",
    "test": "c8 mocha -r test/setup.js test/*.spec.js"
  },
  "dependencies": {
    "@riotjs/register": "^9.1.0",
    "@your-riot/dom-bindings": "workspace:*",
    "@your-riot/utils": "workspace:^",
    "bianco.attr": "^1.1.1",
    "bianco.events": "^1.1.1",
    "bianco.query": "^1.1.4",
    "cumpa": "^2.0.1",
    "rawth": "^3.0.0"
  },
  "devDependencies": {
    "@your-riot/compiler": "workspace:^",
    "@your-riot/register": "workspace:^",
    "@your-riot/rollup-plugin": "workspace:^"
  },
  "peerDependencies": {
    "@your-riot/riot": "workspace:^"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module/index.js",
      "require": "./dist/node/index.cjs"
    },
    "./*": {
      "import": "./dist/module/*.js",
      "require": "./dist/node/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "index.d.ts",
    "index.js",
    "index.umd.js",
    "index.standalone.js",
    "index.standalone.umd.js"
  ],
  "keywords": [
    "riot",
    "Riot.js",
    "router",
    "riot-route",
    "route"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riot/route.git"
  },
  "author": "Gianluca Guarini <gianluca.guarini@gmail.com> (https://gianlucaguarini.com)",
  "license": "MIT"
}
