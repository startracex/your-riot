{
  "excludes": ["node_modules", "dist"],
  "includes": [
    "**/*.ts",
    "**/*.js",
    "**/package.json",
    "biome.json",
    "dprint.json",
    "turbo.json"
  ],
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "printWidth": 80,
    "bracketSpacing": true,
    "trailingComma": "all"
  },
  "plugins": [
    "https://plugins.dprint.dev/prettier-0.58.2.json@2fd8cc9c9773364848dc93735b01a00d55fcfa78a56a9230b4b63c014964c82d"
  ]
}
