    "cross-env": "^7.0.3",
    "dprint": "^0.50.0",
    "glob": "^11.0.3",
    "jsdom": "^26.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^11.7.0",
    "prettier": "^3.5.3",
    "rolldown": "1.0.0-beta.18",
    "rollup": "^4.44.0",
    "sinon": "^21.0.0",
    "sinon-chai": "^4.0.0",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3"
  },
